  a sh:PropertyShape ;
  rdfs:label "label"@nl ;
  sh:datatype rdf:langString ;
  sh:description "Een label voor een object is een voor mensen leesbare naam ter aanduiding van een object."@nl ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:name "label"@nl ;
  sh:nodeKind sh:Literal ;
  sh:path rdfs:label .

skosapnl:BibliographicResource-label
  a sh:PropertyShape ;
  rdfs:label "label"@nl ;
  sh:datatype rdf:langString ;
  sh:description "Een label voor een object is een voor mensen leesbare naam ter aanduiding van een object."@nl ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:minCount 1 ;
  sh:name "label"@nl ;
  sh:nodeKind sh:Literal ;
  sh:path rdfs:label .

  skosapnl:Collection-label
  a sh:PropertyShape ;
  rdfs:label "label"@nl ;
  sh:datatype rdf:langString ;
  sh:description "Een label voor een object is een voor mensen leesbare naam ter aanduiding van een object."@nl ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:minCount 1 ;
  sh:name "label"@nl ;
  sh:nodeKind sh:Literal ;
  sh:path rdfs:label .

    skosapnl:ConceptScheme-label
  a sh:PropertyShape ;
  rdfs:label "label"@nl ;
  sh:datatype rdf:langString ;
  sh:description "Een label voor een object is een voor mensen leesbare naam ter aanduiding van een object."@nl ;
  sh:group skosapnl:LabelsAndDescriptionPropertyGroup ;
  sh:minCount 1 ;
