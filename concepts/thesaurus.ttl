beg-nps:InScheme
  a skos:Concept ;
  skos:inScheme begkr-nps:BegrippenkaderNPS ;
  skos:definition 
    "Relateert een begrip tot het begrippenkader waartoe het behoort"@nl ,
    "Relates a resource (for example a concept) to a concept scheme in which it is included."@en ;
  dct:source
    skos: ,
    <https://www.iso.org/obp/ui/#iso:std:iso:25964:-1:ed-1:v1:en:sec:2.12> ,
    <https://www.w3.org/TR/2009/REC-skos-reference-20090818/#schemes> ;
  skos:prefLabel 
    "is in scheme"@en ,
